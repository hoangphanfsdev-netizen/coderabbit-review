{
  "name": "coderabbit-review",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node dist/server.js",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/server.ts",
    "lint": "eslint . --ext .ts",
    "db:migrate": "node -r dotenv/config ./node_modules/.bin/sequelize-cli db:migrate",
    "db:seed:all": "node -r dotenv/config ./node_modules/.bin/sequelize-cli db:seed:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoangphanfsdev-netizen/coderabbit-review.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hoangphanfsdev-netizen/coderabbit-review/issues"
  },
  "homepage": "https://github.com/hoangphanfsdev-netizen/coderabbit-review#readme",
  "description": "",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.11",
    "prettier": "^3.6.2",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
